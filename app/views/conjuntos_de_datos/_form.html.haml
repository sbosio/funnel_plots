= form_for @conjunto_de_datos do |f|
  - if @conjunto_de_datos.errors.any?
    #error_explanation
      %h2= "#{pluralize(@conjunto_de_datos.errors.count, "error")} prohibited this conjunto_de_datos from being saved:"
      %ul
        - @conjunto_de_datos.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :nombre
    = f.text_field :nombre
  .field
    = f.label :descripcion
    = f.text_field :descripcion
  .field
    = f.label :created_user
    = f.number_field :created_user
  .field
    = f.label :updated_user
    = f.number_field :updated_user
  .actions
    = f.submit 'Save'
